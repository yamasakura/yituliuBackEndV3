<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lhs.mapper.ScheduleMapper">
       <select id="selectPage1" resultType="com.lhs.model.entity.BuildingSchedule" >
           SELECT
               building_schedule.*
           FROM
               building_schedule
                   LIMIT #{page}, #{size}
       </select>

</mapper>